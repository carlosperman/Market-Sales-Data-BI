        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// Obtener hora como número desde campo tipo hora (string como "08:30")
var hora = Time.trim().substring(0, 2);

// Eliminar cero inicial si existe
if (hora.startsWith("0")) {
  hora = hora.substring(1);
}

// Convertir hora a número entero
var horaNum = parseInt(hora, 10);

// Inicializar el campo Hour
var Hour = "Fuera de horario";

if (horaNum >= 6 &amp;&amp; horaNum &lt; 12) {
  Hour = "morning";
} else if (horaNum >= 12 &amp;&amp; horaNum &lt; 18) {
  Hour = "afternoon";
} else if (horaNum >= 18 &amp;&amp; horaNum &lt; 24) {
  Hour = "night";
}

// Obtener Año, Mes, Día desde campo Date (objeto Java Date)
var sdf = new java.text.SimpleDateFormat("yyyy-MM-dd");
var dateStr = sdf.format(Date);

var partes = dateStr.split("-");
var Year = partes[0];
var Month = partes[1];
var Day = partes[2];

SaleAmount = QuantitySold*UnitSellingPrice;

</jsScript_script>
